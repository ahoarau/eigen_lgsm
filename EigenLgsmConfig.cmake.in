@PACKAGE_INIT@

# - Config file for the EigenLgsm package
# It defines the following variables
#  EigenLgsm_INCLUDE_DIRS - include directories for EigenLgsm


# find_package(Eigen REQUIRED)
find_package(PkgConfig)
pkg_search_module(EIGEN3 REQUIRED eigen3)

set_and_check(@PROJECT_NAME@_INCLUDE_DIR "@PACKAGE_INCLUDE_INSTALL_DIR@" "${EIGEN3_INCLUDE_DIRS}")
set(@PROJECT_NAME@_INCLUDE_DIRS "${@PROJECT_NAME@_INCLUDE_DIR}")

message(STATUS "Eigen (+lgsm) include dirs: ${Eigen_INCLUDE_DIR}")
